    "name": "melvyn-node-starter",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/melvynkim/melvyn-node-starter"
    },
    "scripts": {
        "_codecov": "codecov",
        "_heroku": "heroku",
        "build": "yarn build:source && yarn build:spec && yarn build:swagger",
        "build:source": "babel src -d dist --ignore **/__tests__/**,**/__mocks__/**",
        "build:swagger": "node_modules/.bin/swagger-jsdoc -d swagger.json ./src/**/*.js -o swagger.yml",
        "build:spec": "bash ./generate_build_spec.sh",
        "build:zip": "zip -r9 $(node_modules/.bin/jq '.name' package.json | sed 's/\"//g')-$(git rev-parse --short HEAD) dist/ processes.js package.json Dockerfile swagger.yml",
        "e2e": "cross-env JEST_ENV=e2e jest \"src/.*\\.js$\" --forceExit",
        "lint": "node_modules/.bin/esw src/**/*.js --color",
        "lint:fix": "node_modules/.bin/esw src/**/*.js --fix",
        "lint:watch": "yarn lint -- --watch",
        "pre-commit": "yarn run lint:fix",
        "start": "node processes.js",
        "start:develop": "cross-env NODE_ENV=develop yarn build:swagger && yarn build:spec && nodemon src/server.js --exec babel-node",
        "start:local": "cross-env NODE_ENV=local yarn build:swagger && yarn build:spec && nodemon src/server.js --exec babel-node",
        "start:staging": "cross-env NODE_ENV=staging yarn build:swagger && yarn build:spec && nodemon src/server.js --exec babel-node",
        "test": "yarn build && yarn unit && yarn e2e",
        "unit": "jest \"src/.*\\.js$\" --coverage --forceExit"
    },
    "version": "0.0.2"
}